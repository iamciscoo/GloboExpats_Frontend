{
  "name": "expat-marketplace-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Expat Marketplace Frontend - A Next.js application for global expat community marketplace",
  "scripts": {
    "dev": "cross-env NODE_NO_WARNINGS=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "clean": "rm -rf .next out dist",
    "precommit": "npm run lint:check && npm run type-check",
    "postinstall": "echo 'Frontend dependencies installed successfully'",
    "dev:turbo": "cross-env NODE_NO_WARNINGS=1 next dev --turbo"
  },
